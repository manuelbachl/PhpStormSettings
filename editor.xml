<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="10" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bCase\b.*">
      <option name="FOREGROUND" value="5ce2ff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bExpression\b.*">
      <option name="FOREGROUND" value="acff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bCondition\b.*">
      <option name="FOREGROUND" value="acff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bDefinition\b.*">
      <option name="FOREGROUND" value="acff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>